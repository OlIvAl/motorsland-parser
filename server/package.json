    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "18.6.1",
    "@types/user-agents": "1.0.2",
    "bluebird": "3.7.2",
    "brandi": "4.0.2",
    "cors": "2.8.5",
    "cross-fetch": "3.1.5",
    "debug": "4.3.4",
    "express": "4.18.1",
    "fast-csv": "4.3.6",
